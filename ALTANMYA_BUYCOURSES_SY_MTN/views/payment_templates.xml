<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="verified_token_checkmark" inherit_id="payment.verified_token_checkmark">
        <xpath expr="//t[@name='Payment Verified Token Checkmark']" position="inside">
            <t t-if="token.provider_code=='syriatell12'">
                <span class="badge rounded-pill text-bg-warning">syriatell12 Token</span>
            </t>
        </xpath>
    </template>

</odoo>
